{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nest build"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nest start",
        "cwd": "apps/backend"
      }
    },
    "start:dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nest start --watch",
        "cwd": "apps/backend"
      }
    },
    "start:debug": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nest start --debug --watch",
        "cwd": "apps/backend"
      }
    },
    "start:prod": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node dist/main"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "jest"
      }
    },
    "test:watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "jest --watch"
      }
    },
    "test:cov": {
      "executor": "nx:run-commands",
      "options": {
        "command": "jest --coverage"
      }
    },
    "test:debug": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
      }
    },
    "test:e2e": {
      "executor": "nx:run-commands",
      "options": {
        "command": "jest --config ./test/jest-e2e.json"
      }
    },
    "prisma-migrate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma migrate dev --schema=apps/backend/prisma/schema.prisma --config=apps/backend/prisma/prisma.config.ts"
      }
    },
    "prisma-generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma generate --schema=apps/backend/prisma/schema.prisma --config=apps/backend/prisma/prisma.config.ts"
      }
    }
  },
  "tags": []
}
