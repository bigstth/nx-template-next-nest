// Prisma schema for PostgreSQL
// Add your models below

generator client {
    provider   = "prisma-client"
    engineType = "client"
    output     = "./generated"
}

datasource db {
    provider = "postgresql"
}

model Test {
    id        Int      @id @default(autoincrement())
    name      String
    email     String   @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
